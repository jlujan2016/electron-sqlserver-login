{
  "name": "electron-sqlserver-login",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.ejemplo.miaplicacion",
    "productName": "applogin",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "package.json",
      ".env.production",
      "dashboard.html",
      "index.html",
      "preload.js",
      "renderer.js",
      "assets/**/*",
      "styles.css"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": ["deb","AppImage"],
      "icon": "assets/icon.png",
      "category": "Development"
    }
  },
  "keywords": [],
  "author": {
    "name": "Jose Lujan",
    "email": "lujan_exogenesis@hotmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "mssql": "^11.0.1"
  },
  "devDependencies": {
    "electron-builder": "^25.1.8",
    "dotenv": "^16.4.7",
    "electron": "^34.0.2",
    "mssql": "^11.0.1"
  }
}
